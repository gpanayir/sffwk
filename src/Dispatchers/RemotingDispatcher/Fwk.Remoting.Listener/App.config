			<add name="local"  type="local"  baseConfigFile="ConfigurationManeger.xml"  />
		</Providers>
	</FwkConfigProvider>
	<!--Configuracion del logging del frameworck -->
	<FwkLogging>
		<Rules>
			<!--Events:
						2. Error
						3. Warning
						4. Information
				Targets
						1. File
						2. Xml
						3. Database
						4. WindowsEvent
						
			-->
			<add name="Rule2"  events="Error"  target="Xml"  fileName="Logs.xml" />
			<add name="Rule3"  events="Warning"  target="Xml"  cnnStringName="" fileName="Logs.xml"/>
			<add name="Rule4"  events="Information"  target="WindowsEvent" />
			
		</Rules>
	</FwkLogging>
	
	<FwkServiceMetadata defaultProviderName="Xml">
		<Providers>
			<add name="Xml" type="xml" appid ="" sourceinfo="ServiceMetadataConfig.xml" />
			<add name="Sql" type="sqldatabase" appid="" sourceinfo="BPConfig" />
		</Providers>
	</FwkServiceMetadata>
</configuration>