		<add name="test" connectionString="Database=fwktest;Server=(local)\SQLEXPRESS;Integrated Security=SSPI" providerName="System.Data.SqlClient" />

	</connectionStrings>
	<appSettings>
		<add key="MaxRecords" value="2147483647"/>
		<add key="CompanyId" value="pelsoft"/>
	</appSettings>


	<!--Configuracion del los proveedores de  configuracion del frameworck -->
	<FwkConfigProvider defaultProviderName="localFile">
		<Providers>
			
			<add  name ="localFile" type="local"  baseConfigFile="ConfigurationManager.xml"  />
			<add  name ="database" type="sqldatabase" baseConfigFile="ConfigurationManager_BigBang" sourceinfo = "BigBangConnectionString"  />
		</Providers>
	</FwkConfigProvider>
	<!--Configuracion del logging del frameworck -->
	<FwkLogging>
		<Rules>
			
			<add name="Rule2" events="Error" target="Xml" fileName="Logs.xml"/>
			<add name="Rule3" events="Warning" target="Xml" cnnStringName="" fileName="Logs.xml"/>
			<add name="Rule4" events="Information" target="WindowsEvent"/>
		</Rules>
	</FwkLogging>

	<!--Configuracion del metadata de servicios del frameworck -->
	<FwkServiceMetadata defaultProviderName="XML_test">
		<Providers>
			<add name="XML_test" type="xml" appid ="" sourceinfo="..\..\..\..\Services\Fwk.Security.Service.Test\bin\Debug\ServiceMetadataConfig.xml" securityProviderName="sec_bigbang_tesa" />
			<add name="Sql_test" type="sqldatabase"  sourceinfo="test" securityProviderName="sec_bigbang_tesa" />
		</Providers>
	</FwkServiceMetadata>
	
	<FwkWrapper defaultProviderName="bigbang_tesa">
		<Providers>
			<add name="bigbang_tesa" companyId="bigbang_tesa"  serviceMetadataProviderName="XML_test" type="Fwk.Bases.Connector.LocalWrapper,Fwk.Bases.Connector"  sourceinfo="" />
		</Providers>
	</FwkWrapper>
	
	<system.web>
